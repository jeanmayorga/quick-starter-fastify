{
  "name": "quick-starter-fastify",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rm -rf ./build",
    "dev": "tsc-watch --onSuccess \"node build/app.js\"",
    "format": "prettier --write \"src/**/*.ts\" --write \"package.json\"",
    "format:check": "prettier --check \"src/**/*.ts\" --check \"package.json\""
  },
  "dependencies": {
    "@types/node": "^13.11.1",
    "aws-lambda-fastify": "^1.4.3",
    "envalid": "^6.0.1",
    "fastify": "^2.14.1",
    "prettier": "^2.0.4",
    "prettier-plugin-packagejson": "^2.2.0",
    "tsc-watch": "^4.2.3",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3"
  }
}
